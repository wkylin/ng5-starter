{
  "name": "ng-start",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "mock-server-proxy": "concurrently \"nodemon mock/app-server.js\" \"yarn run start-proxy\"",
    "mock-server-multi": "concurrently \"yarn run express-data\" \"yarn run start-proxy-multi\"",
    "build-local": "ng build --env=local -sm=true --output-path=dist-local",
    "build-dev": "ng build --env=dev -sm=true --output-path=dist-dev",
    "build-prod": "ng build --env=prod -sm=true --output-path=dist --verbose=true --stats-json",
    "build": "ng build --prod --aot --extract-css -sm=true --build-optimizer --extract-licenses=false",
    "start-local": "ng serve --env=local --port=4201 --open",
    "start-dev": "ng serve --env=dev --port=4202 --open",
    "start-prod": "ng serve --env=prod --port=4203 --open",
    "start-proxy": "ng serve --host=0.0.0.0 --port=4204 --env=local --open --proxy-config proxy.conf.json",
    "start-proxy-multi": "ng serve --env=local --open --proxy-config proxy.conf.js",
    "bundle-report-stats": "webpack-bundle-analyzer dist/stats.json",
    "bundle-report-explorer": "source-map-explorer dist/main.bundle.js",
    "json-data": "json-server --watch mock/db.json",
    "express-data": "nodemon mock/app-server.js",
    "clean-dev": "del-cli dist-dev",
    "clean-local": "del-cli dist-local",
    "clean-dist": "del-cli dist",
    "ngd": "ngd",
    "hmr": "ng serve --hmr -e=hmr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@ngx-loading-bar/router": "^1.4.0",
    "core-js": "^2.5.3",
    "json-schema-to-typescript": "^5.4.0",
    "ngx-loading": "^1.0.14",
    "normalize.css": "^8.0.0",
    "primeng": "^5.2.3",
    "rxjs": "^5.5.7",
    "webpack-bundle-analyzer": "^2.11.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^9.6.0",
    "angular-in-memory-web-api": "^0.6.0",
    "body-parser": "^1.18.2",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "del-cli": "^1.1.0",
    "express": "^4.16.3",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-server": "^0.12.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "nodemon": "^1.17.2",
    "postcss-sass": "^0.3.0",
    "protractor": "^5.3.0",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.1.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.2"
  }
}
